generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Type {
  TWITTER
  YOUTUBE
  GITHUB
  REDDIT
  WEB
}

model user {
  id            Int       @id @default(autoincrement())
  firstName     String
  lastName      String
  password      String
  verified      Boolean   @default(false)
  session_token String    @default("-1")
  email         String?   @unique
  createdAt     DateTime? @default(now())
}
